<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.tcs.FirstHibernate.MovieTicket" table="TicketBooking" schema="Rituraj">
		<id name="id" column="id">
			<generator class="com.tcs.FirstHibernate.CustomIdGenerator"/>
		</id>
		<property name="Mname" column="mname"/>
		<property name="seatno" column="seatno"/>
		<property name="showtime" column="showtime"/>
		
	</class>

</hibernate-mapping>